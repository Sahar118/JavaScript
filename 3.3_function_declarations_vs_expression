
// EX1

// china
const percentageOfWorld = function(population){
    return `China it's about ${((population*100) / 7900) .toFixed(1)
}% of the world population.`;
};
const result = percentageOfWorld (1441)
console.log(result);

//mexico
const percentageOfWorld = function(population){
    return `Mexico it's about ${((population*100) / 7900) .toFixed(1)
}% of the world population.`;
};
const result = percentageOfWorld (350)
console.log(result);


